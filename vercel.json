{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/health", "dest": "/api/health.js" },
    { "src": "/api/lead", "dest": "/api/lead.js" },
    { "src": "/api/newsletter", "dest": "/api/newsletter.js" },
    { "src": "/api/course-inquiry", "dest": "/api/course-inquiry.js" },
    { "src": "/api/chatbot", "dest": "/api/chatbot.js" },
    { "src": "/api/interviewer", "dest": "/api/interviewer.js" },
    { "src": "/api/candidate", "dest": "/api/candidate.js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
